<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/publicasans"
        android:text="COOK N' CHILL"
        android:textColor="#FF344C"
        android:textSize="30"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.209" />

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:background="@android:color/transparent"
        android:fontFamily="@font/poppins"
        android:stateListAnimator="@null"
        android:text="Login"
        android:textAllCaps="false"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.356" />

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:background="@android:color/transparent"
        android:fontFamily="@font/poppinslight"
        android:stateListAnimator="@null"
        android:text="Sign Up"
        android:textAllCaps="false"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.947"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.356" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="128dp"
        android:fontFamily="@font/poppinsbold"
        android:text="Welcome"
        android:textColor="#1A1A1A"
        android:textSize="49dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.449" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppinslight"
        android:text="You can now enter your credentials to login to \nCook N' Chill and start exploring the cooking world!"
        android:textColor="#1A1A1A"
        android:textSize="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.39"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.537" />

    <ImageView
        android:layout_width="300dp"
        android:layout_height=""
        app:srcCompat="@drawable/path_2467"
        tools:layout_editor_absoluteX="-67dp"
        tools:layout_editor_absoluteY="289dp" />


    <ImageView
        android:layout_width="360dp"
        android:layout_height="219dp"
        app:srcCompat="@drawable/group_16"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="355dp" />

    <EditText
        android:layout_width="291dp"
        android:layout_height="47dp"
        android:background="@android:color/white"
        android:hint="Email Address"
        android:textSize="15dp"
        android:fontFamily="@font/poppinslight"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="434dp" />

    <ImageView
        android:layout_width="360dp"
        android:layout_height="219dp"
        app:srcCompat="@drawable/group_16"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="417dp" />

    <EditText
        android:layout_width="291dp"
        android:layout_height="47dp"
        android:background="@android:color/white"
        android:hint="Password"
        android:textSize="15dp"
        android:fontFamily="@font/poppinslight"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="498dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="300dp"
        android:layout_height="52dp"
        android:background="@drawable/custombutton"
        android:fontFamily="@font/poppisnmedium"
        android:gravity="center"
        android:paddingLeft="10dp"
        android:text="LOGIN"
        android:textAllCaps="false"
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.82" />

    <Button
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:background="@android:color/transparent"
        android:fontFamily="@font/poppinslight"
        android:stateListAnimator="@null"
        android:text="Forgot Password?"
        android:textAllCaps="false"
        android:textColor="#1F54C3"
        android:textSize="14dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="599dp" />

    <ImageView
        android:layout_width="299dp"
        android:layout_height="1dp"
        android:background="@drawable/line_3"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="659dp" />

    <ImageView
        android:layout_width="120dp"
        android:layout_height="70dp"
        android:background="@drawable/union_2"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="659dp" />

    <ImageView
        android:layout_width="24dp"
        android:layout_height="15dp"
        android:background="@drawable/eye"
        tools:layout_editor_absoluteX="316dp"
        tools:layout_editor_absoluteY="515dp" />

    <Button
        android:layout_width="24dp"
        android:layout_height="15dp"
        android:background="@android:color/transparent"
        android:stateListAnimator="@null"
        tools:layout_editor_absoluteX="316dp"
        tools:layout_editor_absoluteY="515dp" />

    <ImageView
        android:layout_width="327dp"
        android:layout_height="234dp"
        android:background="@drawable/rectangle_18"
        android:backgroundTint=""
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="311dp" />

    <TextView
        android:layout_width="400dp"
        android:layout_height="200dp"
        android:text="LOGIN INVALID"
        android:textColor="#4A4A4A"
        android:textSize="26dp"
        tools:layout_editor_absoluteX="108dp"
        tools:layout_editor_absoluteY="417dp" />


</androidx.constraintlayout.widget.ConstraintLayout>